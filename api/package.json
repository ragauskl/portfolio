{
  "name": "api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "s:watch": "concurrently --handle-input \"wait-on dist/main.js && nodemon\" \"tsc -w -p tsconfig.build.json\" ",
    "s:debug": "nodemon --config nodemon-debug.json",
    "s:prod": "node -r ./tsconfig-paths-bootstrap.js dist/main.js",
    "env:dec": "node scripts/decrypt-env.js",
    "env:enc": "secure-env .env -s ${$ENV_DECRYPTION_KEY}",
    "build": "tsc -p tsconfig.build.json",
    "format": "tslint -p tsconfig.json -c tslint.json --fix",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prestart:prod": "rimraf dist && npm run build",
    "docs": "node swagger/index.js"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.7.2",
    "@nestjs/graphql": "^7.3.5",
    "@nestjs/platform-fastify": "^6.7.2",
    "@prtf/shared": "^1.0.0",
    "apollo-server-fastify": "^2.12.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.10.1",
    "cors": "^2.8.5",
    "express-rate-limit": "^5.0.0",
    "fastify": "^2.9.0",
    "graphql": "^15.0.0",
    "graphql-tools": "^5.0.0",
    "moment": "^2.24.0",
    "nest-router": "^1.0.9",
    "nodemailer": "^6.3.0",
    "on-finished": "^2.3.0",
    "on-headers": "^1.0.2",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3",
    "secure-env": "^1.2.0",
    "tsconfig-paths": "^3.8.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/cors": "^2.8.6",
    "@types/express-rate-limit": "^3.3.3",
    "@types/node": "^12.7.11",
    "@types/nodemailer": "^6.2.1",
    "@types/on-finished": "^2.3.1",
    "concurrently": "^4.1.0",
    "cp": "^0.2.0",
    "express": "^4.17.1",
    "fastify-swagger": "^2.4.0",
    "nodemon": "^1.19.3",
    "supertest": "^3.4.2",
    "swagger-ui-express": "^4.1.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.3",
    "wait-on": "^3.3.0"
  }
}
